generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Measure {
  measure_uuid String @id @default(uuid())
  image_url String
  measure_value Int
  customer_code String
  measure_type String
  measure_datetime DateTime
  has_confirmed Boolean
}